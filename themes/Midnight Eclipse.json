  "tokenColors": [
    {
      "settings": {
        "foreground": "#B0BEC5",
        "background": "#121216"
      }
    },
    {
      "name": "Comment color",
      "scope": ["comment", "markup.quote.markdown"],
      "settings": {
        "foreground": "#B0BEC5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword color",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "Function Names",
      "scope": [
        "entity.name.function",
        "meta.object-literal.key.js",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#D67602"
      }
    },
    {
      "name": "Variables and Properties",
      "scope": ["variable", "support.variable", "entity.name.tag"],
      "settings": {
        "foreground": "#9FD808"
      }
    },
    {
      "name": "String literals",
      "scope": ["string", "constant.character"],
      "settings": {
        "foreground": "#0091EA"
      }
    },
    {
      "name": "Numeric Constants",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#D65343"
      }
    },
    {
      "name": "Operators and Punctuation",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#A7A7A7"
      }
    },
    {
      "name": "Operators and Punctuation",
      "scope": ["keyword.operator.logical.js.jsx"],
      "settings": {
        "foreground": "#D60202"
      }
    },
    {
      "name": "Class and Type Names",
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#7BB3F0"
      }
    },
    {
      "name": "Invalid and Errors",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF3D3D",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Brackets and Tags",
      "scope": ["punctuation.definition.tag", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#78D9B2"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["entity.other.attribute-name.css"],
      "settings": {
        "foreground": "#A0A1A7"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["constant.other.css"],
      "settings": {
        "foreground": "#9BBEFF"
      }
    },
    {
      "name": "Italic Text",
      "scope": [
        "comment",
        "meta.property-list.css",
        "keyword",
        "entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2CD7D7"
      }
    },
    {
      "name": "css",
      "scope": [
        "meta.selector.css",
        "punctuation.separator.key-value.css",
        "entity.other.attribute-name.class.css",
        "punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D87308"
      }
    },
    {
      "name": "animation",
      "scope": ["entity.other.keyframe-offset.percentage.css"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0279D6"
      }
    }
  ]
}
